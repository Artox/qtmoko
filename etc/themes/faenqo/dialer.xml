<?xml version="1.0"?>
<page xmlns="http://www.trolltech.com" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://www.trolltech.com themedview.xsd" name="dialer" base="themes/faenqo" keypad="no">

<dialer-lineedit-plugin name="dialernumber" rect="0,0,0x64" bold="no" colorGroup="Background=#000000;Base=#000000;Text=#FFFFFF" bgcolor="Text" size="13"/>
<image rect="0,0,480x64" name="dialnum_bkgr" src="dialernumber.svg" interactive="no" scale="yes" />


    <layout name="vertical" rect="0,64,0,0" orientation="vertical" spacing="-1" align="vcenter">
	<layout name="horizontal" rect="0,0,0x84" orientation="horizontal" spacing="-1" align="hcenter">
	    <group rect="0,0,120x84" name="one" interactive="yes" onclick="message=VirtualKeyboard,keyPress(int),1">
          <image rect="0,0,0,0" name="one_bg" src="button.svg" onclick="src=buttonp.svg" interactive="yes" scale="yes" />
        <text rect="45,8,-45,-37" bold="yes" color="#000000" size="8" align="hcenter,vcenter">1</text>
	    </group>
	    <group rect="0,0,120x84" name="two" interactive="yes" onclick="message=VirtualKeyboard,keyPress(int),2">
          <image rect="0,0,0,0" name="two_bg" src="button.svg" onclick="src=buttonp.svg" interactive="yes" scale="yes" />
          <text rect="45,8,-45,-37" bold="yes" color="#000000" size="8" align="hcenter,vcenter">2</text>
          <text rect="25,-30,-25,-10" bold="no" color="#000000" size="5" align="hcenter,vcenter">ABC</text>
	    </group>
	    <group rect="0,0,120x84" name="three" interactive="yes" onclick="message=VirtualKeyboard,keyPress(int),3">
          <image rect="0,0,0,0" name="three_bg" src="button.svg" onclick="src=buttonp.svg" interactive="yes" scale="yes" />
          <text rect="45,8,-45,-37" bold="yes" color="#000000" size="8" align="hcenter,vcenter">3</text>
          <text rect="25,-30,-25,-10" bold="no" color="#000000" size="5" align="hcenter,vcenter">DEF</text>
	    </group>
	    <group rect="0,0,120x84" name="selectcontact" interactive="yes" onclick="message=Qtopia/Phone/TouchscreenDialer,selectContact()">
          <image rect="0,0,0,0" name="contact_bg" src="generic-personal-contact.png" onclick="src=generic-personal-contact2p.png" interactive="yes" scale="yes" />
	    </group>
	</layout>
	<layout rect="0,0,0x84" orientation="horizontal" spacing="-1" align="hcenter" >
        <group rect="0,0,120x84" name="four" interactive="yes" onclick="message=VirtualKeyboard,keyPress(int),4">
        <image rect="0,0,0,0" name="four_bg" src="button.svg" onclick="src=buttonp.svg" interactive="yes" scale="yes" />
        <text rect="45,8,-45,-37" bold="yes" color="#000000" size="8" align="hcenter,vcenter">4</text>
        <text rect="25,-30,-25,-10" bold="no" color="#000000" size="5" align="hcenter,vcenter">GHI</text>
	    </group>
	    <group rect="0,0,120x84" name="five" interactive="yes" onclick="message=VirtualKeyboard,keyPress(int),5">
          <image rect="0,0,0,0" name="five_bg" src="button.svg" onclick="src=buttonp.svg" interactive="yes" scale="yes" />
        <text rect="45,8,-45,-37" bold="yes" color="#000000" size="8" align="hcenter,vcenter">5</text>
        <text rect="25,-30,-25,-10" bold="no" color="#000000" size="5" align="hcenter,vcenter">JKL</text>
	    </group>
	    <group rect="0,0,120x84" name="six" interactive="yes" onclick="message=VirtualKeyboard,keyPress(int),6">
		<image rect="0,0,0,0" name="six_bg" src="button.svg" onclick="src=buttonp.svg" interactive="yes" scale="yes" />
        <text rect="45,8,-45,-37" bold="yes" color="#000000" size="8" align="hcenter,vcenter">6</text>
        <text rect="25,-30,-25,-10" bold="no" color="#000000" size="5" align="hcenter,vcenter">MNO</text>
	    </group>
	    <group rect="0,0,120x84" name="messages" interactive="yes" onclick="message=Qtopia/Phone/TouchscreenDialer,sms()">
		<image rect="0,0,0,0" name="messages_bg" src="message.png" onclick="src=message2p.png" interactive="yes" scale="yes" />
	    </group>
	</layout>
	<layout rect="0,0,0x84" orientation="horizontal" spacing="-1" align="hcenter" >
	    <group rect="0,0,120x84" name="seven" interactive="yes" onclick="message=VirtualKeyboard,keyPress(int),7">
		<image rect="0,0,0,0" name="seven_bg" src="button.svg" onclick="src=buttonp.svg" interactive="yes" scale="yes" />
        <text rect="45,8,-45,-37" bold="yes" color="#000000" size="8" align="hcenter,vcenter">7</text>
        <text rect="25,-30,-25,-10" bold="no" color="#000000" size="5" align="hcenter,vcenter">PQRS</text>
	    </group>
	    <group rect="0,0,120x84" name="eight" interactive="yes" onclick="message=VirtualKeyboard,keyPress(int),8">
		<image rect="0,0,0,0" name="eight_bg" src="button.svg" onclick="src=buttonp.svg" interactive="yes" scale="yes" />
        <text rect="45,8,-45,-37" bold="yes" color="#000000" size="8" align="hcenter,vcenter">8</text>
        <text rect="25,-30,-25,-10" bold="no" color="#000000" size="5" align="hcenter,vcenter">TUV</text>
	    </group>
	    <group rect="0,0,120x84" name="nine" interactive="yes" onclick="message=VirtualKeyboard,keyPress(int),9">
		<image rect="0,0,0,0" name="nine_bg" src="button.svg" onclick="src=buttonp.svg" interactive="yes" scale="yes" />
        <text rect="45,8,-45,-37" bold="yes" color="#000000" size="8" align="hcenter,vcenter">9</text>
        <text rect="25,-30,-25,-10" bold="no" color="#000000" size="5" align="hcenter,vcenter">WXYZ</text>
	    </group>
	    <group rect="0,0,120x84" name="callhistory" interactive="yes" onclick="message=Qtopia/Phone/TouchscreenDialer,showCallHistory()">
		<image rect="0,0,0,0" name="calls_bg" src="CallHistory.svg" onclick="src=CallHistory2p.svg" interactive="yes" scale="yes" />
	    </group>
	</layout>
	<layout rect="0,0,0x84" orientation="horizontal" spacing="-1" align="hcenter" >
	    <group rect="0,0,120x84" name="star" interactive="yes" onclick="message=VirtualKeyboard,keyPress(int),*">
		<image rect="0,0,0,0" name="star_bg" src="button.svg" onclick="src=buttonp.svg" interactive="yes" scale="yes" />
        <text rect="45,8,-45,-37" bold="yes" color="#000000" size="8" align="hcenter,vcenter">*</text>
        <text rect="25,-30,-25,-10" bold="no" color="#000000" size="5" align="hcenter,vcenter">+</text>
	    </group>
	    <group rect="0,0,120x84" name="zero" interactive="yes" onclick="message=VirtualKeyboard,keyPress(int),0">
		<image rect="0,0,0,0" name="zero_bg" src="button.svg" onclick="src=buttonp.svg" interactive="yes" scale="yes" />
        <text rect="45,8,-45,-37" bold="yes" color="#000000" size="8" align="hcenter,vcenter">0</text>
	    </group>
	    <group rect="0,0,120x84" name="hash" interactive="yes" onclick="message=VirtualKeyboard,keyPress(int),#">
		<image rect="0,0,0,0" name="hash_bg" src="button.svg" onclick="src=buttonp.svg" interactive="yes" scale="yes" />
        <text rect="45,8,-45,-37" bold="yes" color="#000000" size="8" align="hcenter,vcenter">#</text>
	    </group>
	    <group rect="0,0,120x84" name="hangup" interactive="yes" onclick="message=Qtopia/Phone/TouchscreenDialer,hangup()">
		<image rect="0,0,0,0" name="hangup_bg" src="hangup.png" onclick="src=hangup2p.png" interactive="yes" scale="yes" />
	    </group>
	</layout>
    <layout rect="0,0,0x84" orientation="horizontal" spacing="-1" align="hcenter" >
      <group rect="0,0,480x84" name="call" interactive="yes" onclick="message=Qtopia/Phone/TouchscreenDialer,dial()">
        <image rect="0,0,0,0" name="call" src="buttoncall.png" onclick="src=buttoncallp.png" interactive="yes" scale="yes" />
        <text rect="120,0,0,0" bold="yes" outline="#FFFFFF" color="#000000" size="9" align="hcenter,vcenter">Call</text>
      </group>
    </layout>
    </layout>
</page>

<?xml version="1.0"?>
<page xmlns="http://www.trolltech.com" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://www.trolltech.com themedview.xsd" name="dialer" base="themes/finxi" keypad="no">
    <dialer-lineedit-plugin name="dialernumber" rect="0,0,0x48" bold="no" colorGroup="Background=#000000;Base=#000000;Text=#FFFFFF" bgcolor="Text" size="12"/>
    <dialer-lineedit-plugin name="dialercontact" rect="0,48,0x30" bold="no" colorGroup="Background=#000000;Base=#000000;Text=#FFFFFF" bgcolor="Text" size="7"/>
    <layout name="vertical" rect="0,78,0,0" orientation="vertical" spacing="-1" align="vcenter">
	<layout name="horizontal" rect="0,0,0x81" orientation="horizontal" spacing="-1" align="hcenter">
	    <group rect="0,0,120x81" name="one" interactive="yes" onclick="message=VirtualKeyboard,keyPress(int),1">
          <image rect="0,0,0,0" name="one_bg" src="button.svg" onclick="src=buttonp.svg" interactive="yes" scale="yes" />
        <text rect="45,5,-45,-40" bold="yes" outline="#000000" color="#FFFFFF" size="7" align="hcenter,vcenter">1</text>
	    </group>
	    <group rect="0,0,120x81" name="two" interactive="yes" onclick="message=VirtualKeyboard,keyPress(int),2">
          <image rect="0,0,0,0" name="two_bg" src="button.svg" onclick="src=buttonp.svg" interactive="yes" scale="yes" />
          <text rect="45,5,-45,-40" bold="yes" outline="#000000" color="#FFFFFF" size="7" align="hcenter,vcenter">2</text>
          <text rect="25,-40,-25,-20" bold="yes" outline="#000000" color="#888888" size="5" align="hcenter,vcenter">ABC</text>
	    </group>
	    <group rect="0,0,120x81" name="three" interactive="yes" onclick="message=VirtualKeyboard,keyPress(int),3">
          <image rect="0,0,0,0" name="three_bg" src="button.svg" onclick="src=buttonp.svg" interactive="yes" scale="yes" />
          <text rect="45,5,-45,-40" bold="yes" outline="#000000" color="#FFFFFF" size="7" align="hcenter,vcenter">3</text>
          <text rect="25,-40,-25,-20" bold="yes" outline="#000000" color="#888888" size="5" align="hcenter,vcenter">DEF</text>
	    </group>
	    <group rect="0,0,120x81" name="selectcontact" interactive="yes" onclick="message=Qtopia/Phone/TouchscreenDialer,selectContact()">
          <image rect="0,0,0,0" name="contact_bg" src="button.svg" onclick="src=buttonp.svg" interactive="yes" scale="yes" />
          <image rect="40,10,-40,-20" name="contact_fg" src="generic-personal-contact.svg" scale="yes"/>
	    </group>
	</layout>
	<layout rect="0,0,0x81" orientation="horizontal" spacing="-1" align="hcenter" >
        <group rect="0,0,120x81" name="four" interactive="yes" onclick="message=VirtualKeyboard,keyPress(int),4">
        <image rect="0,0,0,0" name="four_bg" src="button.svg" onclick="src=buttonp.svg" interactive="yes" scale="yes" />
        <text rect="45,5,-45,-40" bold="yes" outline="#000000" color="#FFFFFF" size="7" align="hcenter,vcenter">4</text>
        <text rect="25,-40,-25,-20" bold="yes" outline="#000000" color="#888888" size="5" align="hcenter,vcenter">GHI</text>
	    </group>
	    <group rect="0,0,120x81" name="five" interactive="yes" onclick="message=VirtualKeyboard,keyPress(int),5">
          <image rect="0,0,0,0" name="five_bg" src="button.svg" onclick="src=buttonp.svg" interactive="yes" scale="yes" />
        <text rect="45,5,-45,-40" bold="yes" outline="#000000" color="#FFFFFF" size="7" align="hcenter,vcenter">5</text>
        <text rect="25,-40,-25,-20" bold="yes" outline="#000000" color="#888888" size="5" align="hcenter,vcenter">JKL</text>
	    </group>
	    <group rect="0,0,120x81" name="six" interactive="yes" onclick="message=VirtualKeyboard,keyPress(int),6">
		<image rect="0,0,0,0" name="six_bg" src="button.svg" onclick="src=buttonp.svg" interactive="yes" scale="yes" />
        <text rect="45,5,-45,-40" bold="yes" outline="#000000" color="#FFFFFF" size="7" align="hcenter,vcenter">6</text>
        <text rect="25,-40,-25,-20" bold="yes" outline="#000000" color="#888888" size="5" align="hcenter,vcenter">MNO</text>
	    </group>
	    <group rect="0,0,120x81" name="messages" interactive="yes" onclick="message=Qtopia/Phone/TouchscreenDialer,sms()">
		<image rect="0,0,0,0" name="messages_bg" src="button.svg" onclick="src=buttonp.svg" interactive="yes" scale="yes" />
        <image rect="40,20,-40,-20" name="messages_fg" src="message.svg" scale="yes"/>
	    </group>
	</layout>
	<layout rect="0,0,0x81" orientation="horizontal" spacing="-1" align="hcenter" >
	    <group rect="0,0,120x81" name="seven" interactive="yes" onclick="message=VirtualKeyboard,keyPress(int),7">
		<image rect="0,0,0,0" name="seven_bg" src="button.svg" onclick="src=buttonp.svg" interactive="yes" scale="yes" />
        <text rect="45,5,-45,-40" bold="yes" outline="#000000" color="#FFFFFF" size="7" align="hcenter,vcenter">7</text>
        <text rect="25,-40,-25,-20" bold="yes" outline="#000000" color="#888888" size="5" align="hcenter,vcenter">PQRS</text>
	    </group>
	    <group rect="0,0,120x81" name="eight" interactive="yes" onclick="message=VirtualKeyboard,keyPress(int),8">
		<image rect="0,0,0,0" name="eight_bg" src="button.svg" onclick="src=buttonp.svg" interactive="yes" scale="yes" />
        <text rect="45,5,-45,-40" bold="yes" outline="#000000" color="#FFFFFF" size="7" align="hcenter,vcenter">8</text>
        <text rect="25,-40,-25,-20" bold="yes" outline="#000000" color="#888888" size="5" align="hcenter,vcenter">TUV</text>
	    </group>
	    <group rect="0,0,120x81" name="nine" interactive="yes" onclick="message=VirtualKeyboard,keyPress(int),9">
		<image rect="0,0,0,0" name="nine_bg" src="button.svg" onclick="src=buttonp.svg" interactive="yes" scale="yes" />
        <text rect="45,5,-45,-40" bold="yes" outline="#000000" color="#FFFFFF" size="7" align="hcenter,vcenter">9</text>
        <text rect="25,-40,-25,-20" bold="yes" outline="#000000" color="#888888" size="5" align="hcenter,vcenter">WXYZ</text>
	    </group>
	    <group rect="0,0,120x81" name="callhistory" interactive="yes" onclick="message=Qtopia/Phone/TouchscreenDialer,showCallHistory()">
		<image rect="0,0,0,0" name="calls_bg" src="button.svg" onclick="src=buttonp.svg" interactive="yes" scale="yes" />
        <image rect="40,20,-40,-20" name="calls_fg" src="CallHistory.svg" scale="yes"/>
	    </group>
	</layout>
	<layout rect="0,0,0x81" orientation="horizontal" spacing="-1" align="hcenter" >
	    <group rect="0,0,120x81" name="star" interactive="yes" onclick="message=VirtualKeyboard,keyPress(int),*">
		<image rect="0,0,0,0" name="star_bg" src="button.svg" onclick="src=buttonp.svg" interactive="yes" scale="yes" />
        <text rect="45,5,-45,-40" bold="yes" outline="#000000" color="#FFFFFF" size="7" align="hcenter,vcenter">*</text>
        <text rect="25,-40,-25,-20" bold="yes" outline="#000000" color="#888888" size="5" align="hcenter,vcenter">+</text>
	    </group>
	    <group rect="0,0,120x81" name="zero" interactive="yes" onclick="message=VirtualKeyboard,keyPress(int),0">
		<image rect="0,0,0,0" name="zero_bg" src="button.svg" onclick="src=buttonp.svg" interactive="yes" scale="yes" />
        <text rect="45,5,-45,-40" bold="yes" outline="#000000" color="#FFFFFF" size="7" align="hcenter,vcenter">0</text>
	    </group>
	    <group rect="0,0,120x81" name="hash" interactive="yes" onclick="message=VirtualKeyboard,keyPress(int),#">
		<image rect="0,0,0,0" name="hash_bg" src="button.svg" onclick="src=buttonp.svg" interactive="yes" scale="yes" />
        <text rect="45,5,-45,-40" bold="yes" outline="#000000" color="#FFFFFF" size="7" align="hcenter,vcenter">#</text>
	    </group>
	    <group rect="0,0,120x81" name="hangup" interactive="yes" onclick="message=Qtopia/Phone/TouchscreenDialer,hangup()">
		<image rect="0,0,0,0" name="hangup_bg" src="button.svg" onclick="src=buttonp.svg" interactive="yes" scale="yes" />
        <image rect="45,25,-45,-25" name="hangup_fg" src="hangup.svg" scale="yes"/>
	    </group>
	</layout>
    <layout rect="0,0,0x81" orientation="horizontal" spacing="-1" align="hcenter" >
      <group rect="0,0,480x81" name="call" interactive="yes" onclick="message=Qtopia/Phone/TouchscreenDialer,dial()">
        <image rect="0,0,0,0" name="call" src="buttoncall.svg" onclick="src=buttonp.svg" interactive="yes" scale="yes" />
        <text rect="0,0,0,0" bold="yes" outline="#000000" color="#FFFFFF" size="7" align="hcenter,vcenter">Call</text>
      </group>
    </layout>
    </layout>
</page>
